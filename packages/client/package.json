{
  "name": "@actor-core/client",
  "version": "0.0.1",
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "deno": "./src/mod.ts",
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    },
    "./test": {
      "deno": "./src/test.ts",
      "import": {
        "types": "./dist/test.d.ts",
        "default": "./dist/test.js"
      },
      "require": {
        "types": "./dist/test.d.cts",
        "default": "./dist/test.cjs"
      }
    },
    "./unstable-react": {
      "deno": "./src/unstable-react.tsx",
      "import": {
        "types": "./dist/unstable-react.d.ts",
        "default": "./dist/unstable-react.js"
      },
      "require": {
        "types": "./dist/unstable-react.d.cts",
        "default": "./dist/unstable-react.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/mod.ts src/test.ts src/unstable-react.tsx",
    "dev": "yarn build --watch",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "cbor-x": "^1.6.0",
    "zod": "^3.24.1"
  },
  "optionalDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@actor-core/actor-protocol": "workspace:*",
    "@actor-core/common": "workspace:*",
    "@actor-core/manager-protocol": "workspace:*",
    "@kentcdodds/tmp-react-server-dom-esm": "^19.0.1",
    "@types/react": "^19.0.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
