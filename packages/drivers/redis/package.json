  },
  "peerDependencies": {
    "actor-core": "workspace:*"
  },
  "devDependencies": {
    "@actor-core/driver-test-suite": "workspace:*",
    "@types/node": "^22.13.1",
    "actor-core": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.5.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@types/node": "^22.13.1",
    "dedent": "^1.5.3",
    "hono": "^4.7.0",
    "ioredis": "^5.4.2",
    "p-retry": "^6.2.1",
    "zx": "^7.2.3"
  }
}
