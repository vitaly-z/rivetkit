{
  "name": "linear-coding-agent",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw \"yarn dev:actors\" \"yarn dev:server\" \"yarn dev:ngrok\"",
    "dev:actors": "npx @actor-core/cli@latest dev src/actors/app.ts",
    "dev:server": "tsx --watch src/server/index.ts",
    "dev:ngrok": "ngrok http 3000",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@actor-core/cli": "workspace:*",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5",
    "@types/node": "^22.13.9",
    "@types/prompts": "^2",
    "actor-core": "workspace:*",
    "concurrently": "^9.1.2",
    "prompts": "^2.4.2",
    "tsx": "^3.12.7",
    "typescript": "^5.5.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@hono/node-server": "^1.14.1",
    "@linear/sdk": "^7.0.0",
    "@octokit/rest": "^19.0.13",
    "ai": "^4.3.9",
    "body-parser": "^2.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "hono": "^4.7.7",
    "linear-webhook": "^0.1.3",
    "zod": "^3.24.3"
  },
  "example": {
    "platforms": [
      "*"
    ]
  }
}
